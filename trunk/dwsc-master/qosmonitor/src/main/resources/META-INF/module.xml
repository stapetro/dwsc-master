<module name="qosmonitor" class="bg.unisofia.fmi.dwsc.qosmonitoring.QoSMontirongModule">
    <InFlow>
        <handler name="FlowQosMonitorHandler" class="bg.unisofia.fmi.dwsc.qosmonitoring.QosMonitoringHandler">
            <order phase="qosmonitorPhase"/>
        </handler>
    </InFlow>

    <OutFlow>
        <handler name="FlowQosMonitorHandler" class="bg.unisofia.fmi.dwsc.qosmonitoring.QosMonitoringHandler">
            <order phase="qosmonitorPhase"/>
        </handler>
    </OutFlow>

    <OutFaultFlow>
        <handler name="FaultQosMonitorHandler" class="bg.unisofia.fmi.dwsc.qosmonitoring.FaultQosMonitoringHandler">
            <order phase="qosmonitorPhase"/>
        </handler>
    </OutFaultFlow>

    <InFaultFlow>
        <handler name="FaultQosMonitorHandler" class="bg.unisofia.fmi.dwsc.qosmonitoring.FaultQosMonitoringHandler">
            <order phase="qosmonitorPhase"/>
        </handler>
    </InFaultFlow>
</module>
